snippet     stdjareport
abbr        stdjareport template
options     head
	@require: stdjareport
	@require: mylib/common

	document(|
		title={${1:#:title}};
		author={${2:#:author}};
	|) '<
		${0}
	>

snippet	slydifi
abbr	slydifi template
options	head
	@require: class-slydifi/theme/hakodate-custom

	SlydifiHakodate.document(|
		draft-mode = false;
	|) '<
		+frame{${1:#:フレームのタイトル}} <
			${0}
		>
	>
