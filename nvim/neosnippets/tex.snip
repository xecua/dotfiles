snippet     jlreq
abbr        jlreq template
alias       documentclass jlreq
option      head
	\documentclass{jlreq}
	\usepackage{mymacros}
	\usepackage[haranoaji]{luatexja-preset} % if LuaLaTeX
	\renewcommand{\thesection}{\arabic{section}}
	\renewcommand{\thesubsection}{\alph{subsection}}
	\renewcommand{\labelenumi}{\alph{enumi}}
	\renewcommand{\labelenumii}{\arabic{enumii}.}
	
	\makeatletter
	\def\id#1{\def\@id{#1}}
	\def\other#1{\def\@other{#1}}
	
	\def\@maketitle{
		\begin{center}
		\vspace*{10mm}
		{\huge \textbf{\textgt{\@title}} \par}
		\vspace{20mm}
		{\LARGE \textbf{\textgt{\@id \@author}} \par}
		\vspace{10mm}
		% 提出日: \@date\par
		% \@other
		% \newpage
		\end{center}
	}
	\makeatother
	
	\author{${1:author}}
	\id{${2:id}}
	% \date{}
	\title{${3:title}}
	% \other{}
	\defrhead{}
	
	\begin{document}
	\maketitle
	${0}
	
	% \begin{thebibliography}{9}
	% \end{thebibliography}
	\end{document}

snippet     article
