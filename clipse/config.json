{
    "allowDuplicates": false,
    "historyFile": "$XDG_STATE_HOME/clipse/clipboard_history.json",
    "maxHistory": 100,
    "logFile": "$XDG_STATE_HOME/clipse/clipse.log",
    "themeFile": "custom_theme.json",
    "tempDir": "$XDG_STATE_HOME/clipse/tmp_files",
    "keyBindings": {
        "choose": "enter",
        "clearSelected": "S",
        "down": "j",
        "end": "end",
        "filter": "/",
        "home": "home",
        "more": "?",
        "nextPage": "k",
        "prevPage": "j",
        "preview": " ",
        "quit": "q",
        "remove": "x",
        "selectDown": "ctrl+down",
        "selectSingle": "s",
        "selectUp": "ctrl+up",
        "togglePin": "p",
        "togglePinned": "tab",
        "up": "k",
        "yankFilter": "ctrl+s"
    },
    "imageDisplay": {
        "type": "kitty",
        "scaleX": 9,
        "scaleY": 9,
        "heightCut": 2
    }

}
